{
  "name": "chubb-studio-servicing",
  "version": "2.5.9-rc.67",
  "scripts": {
    "ng": "ng",
    "yarn": "yarn",
    "build": "ng build",
    "build:dev": "ng build --configuration dev",
    "build:sit": "ng build --configuration sit --base-href /myaccount --deploy-url /myaccount/",
    "build:sandbox": "ng build --configuration sandbox",
    "build:uat": "ng build --prod --configuration uat --base-href /myaccount --deploy-url /myaccount/",
    "build:prod": "ng build --prod --configuration production --base-href /myaccount --deploy-url /myaccount/",
    "build:stats": "ng build --prod --build-optimizer --stats-json",
    "build:sourcemaps": "ng build --prod --build-optimizer --sourcemaps",
    "build:la-sit": "ng build --configuration la-sit --base-href /myaccount --deploy-url /myaccount/",
    "build:la-uat": "ng build --prod --configuration la-uat --base-href /myaccount --deploy-url /myaccount/",
    "build:la-prod": "ng build --prod --configuration la-production --base-href /myaccount --deploy-url /myaccount/",
    "build:sit-kr": "ng build --configuration sit-kr --base-href /myaccount --deploy-url /myaccount/",
    "build:uat-kr": "ng build --prod --configuration uat-kr --base-href /myaccount --deploy-url /myaccount/",
    "build:prod-kr": "ng build --prod --configuration production-kr --base-href /myaccount --deploy-url /myaccount/",
    "crux:metrics": "node tools/crux/metrics.js",
    "lint": "yarn run lint:styles && ng lint && yarn run lint:html",
    "lint:styles": "stylelint \"./**/*.scss\" --fix",
    "lint:html": "html-linter --config ./node_modules/@crux/config/htmllint/htmllint.json ./src/app/**/*.html",
    "postbuild:dev": "yarn run studio:version:update",
    "postbuild:sandbox": "yarn run studio:version:update",
    "prestart": "yarn run studio:prestart",
    "prestart:proxy": "yarn run studio:prestart",
    "prebuild": "yarn run lint:styles && yarn run studio:prestart",
    "prebuild:dev": "yarn run studio:prestart",
    "prebuild:sit": "yarn run studio:prestart",
    "prebuild:sandbox": "yarn run studio:prestart",
    "prebuild:uat": "yarn run studio:prestart",
    "prebuild:prod": "yarn run studio:prestart",
    "prebuild:sit-kr": "yarn run studio:prestart",
    "prebuild:uat-kr": "yarn run studio:prestart",
    "prebuild:prod-kr": "yarn run studio:prestart",
    "start": "ng serve --base-href /myaccount --servePath /myaccount/ --deploy-url /myaccount/",
    "start:jp": "ng serve --base-href /myaccount/jp --servePath /myaccount/jp --deploy-url /myaccount/jp",
    "start:kr": "yarn start --configuration kr",
    "start:proxy": "yarn start --proxy-config proxy.config.json",
    "start:emea": "yarn start --proxy-config proxy-emea.config.json",
    "start:la": "yarn start --proxy-config proxy-la.config.json",
    "start:stats": "yarn run build:stats && webpack-bundle-analyzer dist/myaccount/stats-es2015.json",
    "studio:app-context": "node ./tools/studio/app-context-builder.js",
    "studio:prestart": "yarn run studio:app-context && yarn run copy-fnol-forms",
    "studio:version:generate": "crux-toolkit-update-version -m create",
    "studio:version:update": "crux-toolkit-update-version -m update-index",
    "test": "yarn run test:jest && yarn run test:e2e",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:jest": "jest --outputFile=test-results/chubb-studio-unit-test-report.json --json --silent --coverage --coverageDirectory=test-results",
    "test:e2e": "yarn run test:cypress:headless",
    "test:cypress:headless": "start-server-and-test test:cypress:start-server http-get://localhost:4201/ test:cypress:run",
    "test:cypress:headed": "start-server-and-test test:cypress:start-server http://localhost:4201/ test:cypress:open",
    "test:cypress:run": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:start-server": "yarn start -- --port=4201 --open",
    "test:coverage": "yarn run test -- --coverage",
    "version:as": "standard-version --release-as",
    "version:patch": "standard-version --release-as patch",
    "version:major": "standard-version --release-as major",
    "version:minor": "standard-version --release-as minor",
    "release:major": "yarn run version:major && yarn run build:prod",
    "release:minor": "yarn run version:minor && yarn run build:prod",
    "release:patch": "yarn run version:patch && yarn run build:prod",
    "preinstall": "node ./tools/check-yarn.js",
    "postinstall": "ngcc",
    "create-npmrc": "node ./tools/create-yarnrc.js",
    "copy-fnol-forms": "node ./tools/copy-fnol-forms.js"
  },
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && yarn run lint"
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "yarn run studio:version:generate && git add ./src/app/version.ts && git commit -m \"chore(scf-9062): update version\" ",
      "release": "standard-version -a"
    }
  },
